<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   android:id="@+id/constraintLayout5"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   android:background="@color/black_two">

    <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline49"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/new_calender_viewPage"
            app:layout_constraintGuide_begin="56dp"
            app:layout_constraintTop_toBottomOf="@+id/time_tabLayout"
            app:layout_constraintVertical_bias="0.0"/>


    <com.google.android.material.tabs.TabLayout
            android:id="@+id/time_tabLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:tabGravity="center"
            app:tabIndicatorColor="@android:color/white"
            app:tabMode="scrollable"
            app:tabSelectedTextColor="@color/white"
            app:tabTextAppearance="@style/AppTabTextForSlotAppearance"
            app:tabTextColor="@color/white">


    </com.google.android.material.tabs.TabLayout>


    <com.royalenfield.reprime.ui.riderprofile.viewpager.RiderProfileViewPager
            android:id="@+id/new_calender_viewPage"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/guideline49">


    </com.royalenfield.reprime.ui.riderprofile.viewpager.RiderProfileViewPager>

</androidx.constraintlayout.widget.ConstraintLayout>
